<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
      <table style="width:100%">
        <tr>
          <th>Firstname</th>
          <th>Lastname</th> 
          <th>Birth Year</th>
        </tr>
         <% inventors.forEach(function(inventor) { %>
          <tr>
            <td><%= inventor.first %></td>
            <td><%= inventor.last %></td> 
            <td><%= inventor.year %></td>
          </tr>
        <% }); %>
      </table>
  </body>
</html>
